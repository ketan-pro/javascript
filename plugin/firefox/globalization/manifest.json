{
    "manifest_version": 2,
    "name": "Globalization",
    "version": "1.0",
    "description": "Globalization extension to mark selected strings and take screenshots.",
    "browser_action": {
        "default_icon": {
          "38": "globe.jpg"
        },
        "default_title": "Globalization",
        "default_popup": "popup/menu.html"
    },
    "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "browserSettings",
    "browsingData",
    "contextMenus",
    "contextualIdentities",
    "cookies",
    "dns",
    "downloads",
    "downloads.open",
    "find",
    "geolocation",
    "history",
    "identity",
    "idle",
    "management",
    "menus",
    "nativeMessaging",
    "notifications",
    "pkcs11",
    "privacy",
    "proxy",
    "search",
    "sessions",
    "storage",
    "tabHide",
    "tabs",
    "theme",
    "topSites",
    "webNavigation",
    "webRequest",
        "<all_urls>"
    ],
    "icons": {
        "48": "globe.jpg"
    }
}